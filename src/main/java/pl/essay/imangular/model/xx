select item1_.id as col_0_0_, item1_.name as col_1_0_, item1_.where_manufactured as col_2_0_, 

item1_.other_sources as col_3_0_, billofmate0_.required_quantity as col_4_0_, billofmate0_.effective_required_quantity as col_5_0_, 

stocks3_.in_stock_quantity as col_6_0_, stocks3_.id as col_7_0_ 

from bill_of_material_flat_list_line billofmate0_ 

inner join item item1_ on billofmate0_.for_item_id=item1_.id 

inner join bill_of_material billofmate2_ on billofmate0_.bom_id=billofmate2_.id 

left outer join bill_of_material_in_stock stocks3_ on billofmate2_.id=stocks3_.bom_id 


where billofmate2_.id=5 and ( stocks3_.for_item_id == null or stocks3_.for_item_id=item1_.id) order by item1_.name
